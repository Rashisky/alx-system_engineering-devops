[Unit]
Description=Starts gunicorn on system startup
After=network.target

[Service]
User=rashisky
Group=rashisky
WorkingDirectory=/home/rashisky/AirBnB_clone_v4/
ExecStart=/usr/bin/gunicorn --workers 3 --bind 0.0.0.0:5003 web_dynamic.2-hbnb:app >> /tmp/airbnb-access.log 2> /tmp/airbnb-error.log

[Install]
WantedBy=multi-user.target
